#bin/bash

cp ./firmware/boot.py /Volumes/CIRCUITPY/boot.py
cp ./firmware/code.py /Volumes/CIRCUITPY/code.py
cp ./firmware/main.py /Volumes/CIRCUITPY/main.py
cp ./firmware/flash.py /Volumes/CIRCUITPY/flash.py
cp ./firmware/layout.json /Volumes/CIRCUITPY/layout.json
cp ./firmware/utils.py /Volumes/CIRCUITPY/utils.py
cp ./firmware/constants.py /Volumes/CIRCUITPY/constants.py
cp ./firmware/config.py /Volumes/CIRCUITPY/config.py
cp -r ./firmware/lib/ /Volumes/CIRCUITPY/lib/
cp -r ./firmware/modules/ /Volumes/CIRCUITPY/modules/
cp -r ./firmware/controllers/ /Volumes/CIRCUITPY/controllers/
